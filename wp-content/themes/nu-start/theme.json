    "elements": {
      "link": {
        "typography": {
          "fontWeight": "var(--wp--custom--font-weight--normal)"
        }
      },
      "h1": {
        "typography": {
          "fontWeight": "400",
          "fontFamily": "Lato",
          "lineHeight": "1.375",
          "fontSize": "var(--wp--preset--font-size--48-48)"
        }
      },
      "h2": {
        "typography": {
          "fontWeight": "400",
          "fontFamily": "Lato",
          "lineHeight": "1.375",
          "fontSize": "var(--wp--preset--font-size--36-40)"
        }
      },
      "h3": {
        "typography": {
          "fontWeight": "400",
          "fontFamily": "Lato",
          "lineHeight": "1.375",
          "fontSize": "var(--wp--preset--font-size--30-36)"
        }
      },
      "h4": {
        "typography": {
          "fontWeight": "700",
          "fontFamily": "Lato",
          "lineHeight": "1.375",
          "fontSize": "var(--wp--preset--font-size--20-28)"
        }
      },
      "h5": {
        "typography": {
          "fontWeight": "700",
          "fontFamily": "Lato",
          "lineHeight": "1.375",
          "fontSize": "var(--wp--preset--font-size--16-24)"
        }
      },
      "h6": {
        "typography": {
          "fontWeight": "700",
          "fontFamily": "Lato",
          "lineHeight": "1.375",
          "fontSize": "var(--wp--preset--font-size--16-24)",
          "textTransform": "uppercase"
        }
      }
    },
    "blocks": {
      "core/navigation-link": {
        "spacing": {
          "blockGap": "0"
        }
      },
      "core/navigation": {
        "spacing": {
          "blockGap": "1rem"
        }
      },
      "core/button": {
        "border": {
          "radius": "0",
          "width": "2px"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--14-20)",
          "lineHeight": "1"
        },
        "spacing": {
          "padding": {
            "top": "1rem",
            "right": "2rem",
            "bottom": "1rem",
            "left": "2rem"
          }
        }
      },
      "core/paragraph": {
        "typography": {
          "lineHeight": "1.375",
          "fontSize": "var(--wp--preset--font-size--16-24)"
        },
        "elements": {
          "link": {
            "typography": {
              "fontWeight": "var(--wp--custom--font-weight--normal)"
            }
          }
        }
      }
    }
