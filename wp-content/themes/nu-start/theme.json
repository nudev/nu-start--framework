{
  "$schema": "https://json.schemastore.org/theme-v1.json",
  "version": 1,
  "settings": {
    "border": {
      "customColor": true,
      "customRadius": true,
      "customStyle": true,
      "customWidth": true
    },
    "color": {
      "duotone": [],
      "gradients": [],
      "link": true,
      "palette": [
        {
          "name": "Warm Gray",
          "slug": "warm-gray",
          "color": "#4D4A45"
        },
        {
          "name": "Black",
          "slug": "black",
          "color": "#000000"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#ffffff"
        },
        {
          "name": "Red",
          "slug": "red",
          "color": "#d41b2c"
        },
        {
          "name": "Blue",
          "slug": "blue",
          "color": "#4299e1"
        },
        {
          "name": "Dark Blue",
          "slug": "dark-blue",
          "color": "#385d85"
        },
        {
          "name": "Orange",
          "slug": "orange",
          "color": "#f86716"
        },
        {
          "name": "Yellow",
          "slug": "yellow",
          "color": "#fac515"
        },
        {
          "name": "Green",
          "slug": "green",
          "color": "#48bb78"
        },
        {
          "name": "Transparent",
          "slug": "transparent",
          "color": "transparent"
        }
      ]
    },
    "layout": {
      "contentSize": "80%",
      "wideSize": "90%"
    },
    "spacing": {
      "customMargin": true,
      "customPadding": true
    },
    "typography": {
      "customFontSize": true,
      "customLineHeight": true,
      "fontFamilies": [
        {
          "fontFamily": "Lato, sans-serif",
          "slug": "lato"
        }
      ],
      "fontSizes": [
        {
          "slug": "tiny",
          "size": "0.75rem",
          "name": "Tiny"
        },
        {
          "slug": "smaller",
          "size": "0.875rem",
          "name": "Smaller"
        },
        {
          "slug": "normal",
          "size": "1rem",
          "name": "Normal"
        },
        {
          "slug": "larger",
          "size": "1.125rem",
          "name": "Larger"
        },
        {
          "slug": "large",
          "size": "1.5rem",
          "name": "Large"
        },
        {
          "slug": "extra-large",
          "size": "2rem",
          "name": "Extra Large"
        },
        {
          "slug": "huge",
          "size": "3rem",
          "name": "Huge"
        },
        {
          "slug": "hugest",
          "size": "5rem",
          "name": "Hugest"
        }
      ]
    },
    "custom": {
      "font-primary": "Lato, sans-serif",
      "line-height": {
        "body": 1.375,
        "heading": 1.375,
        "page-title": 1.5
      },
      "spacing": {
        "unit": "16px",
        "horizontal": "32px",
        "vertical": "32px"
      },
      "font-weight": {
        "light": "300",
        "normal": "400"
      }
    },
    "blocks": {
      "core/cover": {
        "spacing": {
          "units": ["%", "px", "vw"]
        }
      }
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--warm-gray)",
      "background": "var(--wp--preset--color--white)"
    },
    "elements": {
      "link": {
        "typography": {
          "fontWeight": "var(--wp--custom--font-weight--normal)"
        }
      },
      "h1": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "typography": {
          "fontWeight": "400",
          "fontFamily": "Lato",
          "lineHeight": "1.5",
          "fontSize": "48px"
        }
      },
      "h2": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "typography": {
          "fontWeight": "400",
          "fontFamily": "Lato",
          "lineHeight": "1.5",
          "fontSize": "36px"
        }
      },
      "h3": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "typography": {
          "fontWeight": "400",
          "fontFamily": "Lato",
          "lineHeight": "1.5",
          "fontSize": "30px"
        }
      },
      "h4": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "typography": {
          "fontWeight": "700",
          "fontFamily": "Lato",
          "lineHeight": "1.5",
          "fontSize": "20px"
        }
      },
      "h5": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "typography": {
          "fontWeight": "700",
          "fontFamily": "Lato",
          "lineHeight": "1.5",
          "fontSize": "16px"
        }
      },
      "h6": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "typography": {
          "fontWeight": "700",
          "fontFamily": "Lato",
          "lineHeight": "1.5",
          "fontSize": "16px",
          "textTransform": "uppercase"
        }
      }
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": ""
        }
      },
      "core/paragraph": {
        "color": {
          "text": "var(--wp--preset--color--warm-gray)"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--warm-gray)"
            },
            "typography": {
              "fontWeight": "var(--wp--custom--font-weight--normal)"
            }
          }
        }
      }
    }
  }
}
